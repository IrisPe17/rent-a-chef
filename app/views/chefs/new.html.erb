<div>
  <h1 class="text-center">Create a new chef profile</h1>
</div>

<div class="container col-3">
    <%= simple_form_for(@chef) do |f| %>
      <%= f.input :name %>
      <%= f.input :location %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.submit %> <%= link_to 'Back', chefs_path %>
    <% end %>
</div>

